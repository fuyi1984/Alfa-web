<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alfa.web.dao.SysRoleMapper">

    <insert id="insertRole" parameterType="com.alfa.web.pojo.Role">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Long">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into role(role_name,types,status,create_date,bycreater_id)
        value(#{role_name},#{types},#{status},#{create_date},#{bycreater_id})
    </insert>

    <select id="findRole" resultType="com.alfa.web.pojo.Role">
        SELECT * FROM role
    </select>
</mapper>